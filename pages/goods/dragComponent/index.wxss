.item-wrap {
position: relative; }
.item-wrap .item {
position: absolute;
width: 100%;
z-index: 1; 
box-sizing: border-box;
padding: 5rpx;
}
.item-wrap .item.itemTransition {
transition: transform 0.3s !important; }
.item-wrap .item.zIndex {
z-index: 2; }
.item-wrap .item.cur {
transition: initial; }
.item-wrap .item.fixed {
z-index: 0 !important; }

.info {
position: relative;
padding-top: 100%;
background: #ffffff; }
.info__item {
position: absolute;
/* border: 1rpx solid #ccc; */
top: 0;
left: 0;
width: 100%;
height: 100%;
overflow: hidden;
padding: 10rpx;
box-sizing: border-box; 
}
.info__item .image {
width: 100%;
height: 100%; }

.cell {
display: flex;
padding: 20rpx;
border-bottom: 1rpx solid #ebedf0;
background: #FFFFFF; }
.cell__hd {
font-size: 0; }
.cell__hd .image {
width: 160rpx;
height: 160rpx;
margin-right: 20rpx;
border-radius: 12rpx; }
.cell__bd {
flex: 1; }
.cell__bd .name {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  word-break: break-all;
  font-size: 28rpx;
  margin-bottom: 4rpx; 
}
.cell__bd .des {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  word-break: break-all;
  color: #666666;
  font-size: 24rpx; 
}
.itemInput {
  width: 100%;
  font-size: 20rpx;
  color: #000;
  border: 2rpx solid #18c2ba;
  text-align: center;
  background-color: #f2f2f2;
  box-sizing: border-box;
}
