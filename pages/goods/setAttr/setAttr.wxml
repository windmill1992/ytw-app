<view class="titleBox rela">
  <view class="titleName">
    <view style="color:{{ curLevel ==1 ? '#18c2ba' : '' }};" bindtap="chooseTitle" data-type="1">{{ name1 || '一级分类' }}</view>
    <view style="color:{{ curLevel ==2 ? '#18c2ba' : '' }};" bindtap="chooseTitle" data-type="2">{{ name2 || (curLevel ==2 ? (name2 || '二级分类') : '') }}</view>
    <view style="color:{{ curLevel ==3 ? '#18c2ba' : '' }};" bindtap="chooseTitle" data-type="3">{{ name3 || (curLevel ==3 ? (name3 || '三级分类') : '') }}</view>
  </view>
  <view class="titleBorder abs" style="left:{{ borderLeft }}"><view></view></view>
</view>
<view class="categoryBox">
  <van-cell title="{{ item.name }}" is-link bind:click="chooseCate"  wx:for="{{CategoryList1}}" wx:key="index" data-level="{{ item.level }}" data-id="{{ item.id }}" data-name="{{ item.name }}"/>
</view>