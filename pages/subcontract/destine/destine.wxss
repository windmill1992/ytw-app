page{
  background: #eee;
}
.container13 {
  width: 100%;
  display: flex;
  align-items: center;
  overflow-x: hidden;
  flex-direction: column;
  padding-top: 12%;
}
.banner{
  position: fixed;
  top: 0;
  left: 0;
  z-index: -1;
  width: 100%;
  height: 393rpx;
}
.logo{
  width: 140rpx;
  height: 140rpx;
  border-radius: 100%;
}
.right{
  width: 18rpx;
  height: 22rpx;
}
.list{ 
  width: 95%;
  height: auto;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
}
.item{position: relative;
  margin-top: 8%;
  width: 100%;
  border-radius: 10rpx;
  background: #3abdb7;
  box-shadow: 4rpx 4rpx 8rpx #3abdb7;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  letter-spacing: 6rpx;
  color: #fff;
  padding: 10rpx 5%;
  box-sizing: border-box;
  margin-bottom: 30rpx;
  overflow: hidden;
}
.item::after {
  content: '';
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%) rotate(45deg);
  height: 35rpx;
  width: 35rpx;
  border-top: 2rpx solid #fff;
  border-right: 2rpx solid #fff;
  background-color: transparent;
}
.tip{
  font-size: 24rpx;
  color: #333;
  text-align: center;
}

.nav {
  height: 160rpx;
  position: relative;
  width: 100%;
  color: #fff;
  text-align: center;
  font-size: 36rpx;
}
.nav .back {
  position: absolute;
  left: 30rpx;
  top: 70rpx;
  font-size: 36rpx;
  z-index: 105;
}
.nav .tit {
  position: absolute;
  left: 50%;
  top: 66rpx;
  transform: translateX(-50%);
  z-index: 105;
}


