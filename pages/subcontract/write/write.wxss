page {
  font-family: 'Arial Narrow', Arial, sans-serif;
}
.cangPicker {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 750rpx;
  z-index: 10;
}
.van-icon__image {
  vertical-align: top;
}
.van-row {
  background-color: #fff;
}

.textarea{
  position: relative;
  padding-left: 85rpx;
  width: 100%;
  height: 55rpx;
  font-size: 28rpx;
  border-radius: 20rpx;
  border: 1rpx solid #18c2ba;
  box-sizing: border-box;
}
.textarea::before {
  content: '备注:';
  position: absolute;
  left: 15rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size: 28rpx;
  color: #444;
  font-weight: 500;
}
.btn,.btn2{
  border: none;
  margin: 30rpx auto;
  width: 68%;
  background: #3abdb7;
  font-size: 32rpx;
  color: #fff;
  border-radius: 14rpx;
  box-shadow: 4rpx 4rpx 8rpx #3abdb7;
}
.btn2 {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  z-index: 102;
}

.van-icon-close:before {
content: "确定"!important;
color: #555!important;
  } 
.company-check {
  padding-left: 20rpx;
}

/* 预览 */
.previewBox {
  padding: 0 0 0 35rpx;
  color: #999;
}
.previewBox view {
  padding: 15rpx 0;
}
.col3 {
  color: #333;
}

/* 历史厂家 */
.hisTelNum {
  color: #18c2ba;
}
.pdl15 {
  padding-left: 15rpx;
}
.addHisTel {
  color: #fff;
  background-color: #18c2ba;
  padding: 3rpx 10rpx;
  border-radius: 8rpx;
  font-size: 24rpx;
}
.teladdtips {
  color: #18c2ba;
  font-size: 20rpx;
}
.editInput {
  margin: 0 auto;
  width: 70%;
  background-color: rgb(246, 239, 239);
  border-radius: 20rpx;
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
}

.shooseWuliu {
  position: relative;
  font-size: 29rpx;
  padding: 0 16px;
  padding-left: 36px;
  background: #fff;
  color: var(--cell-text-color,#323233);
  box-sizing: border-box;
}
.shooseWuliuBox {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  margin: 0 auto;
  padding: 18rpx 0;
  width: 100%;
  border-bottom: 1rpx solid rgb(218, 211, 211);
  box-sizing: border-box;
}
.van-cell__title {
  color: #444;
  font-weight: bold;
  font-size: 30rpx;
}
.blodfont {
  margin-right: 32px;
  color: #444;
  font-weight: bold;
  font-size: 30rpx;
}
.shooseWuliu .van-radio__label {
  margin-left: 0!important;
  margin-right: 10px!important;
}
.addressBox {
  position: relative;
  padding: 8rpx 45rpx 8rpx 250rpx;
  box-sizing: border-box;
  color: #323233;
  font-size: 27rpx;
  background-color: #fff;
}
.addressBox::before {
  content: '收货地址';
  position: absolute;
  left: 38px;
  top: 50%;
  font-size: 30rpx;
  font-weight: bold;
  color: #444;
  transform: translateY(-50%);
}
.addressBox::after {
  content: '';
  height: 25rpx;
  width: 25rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  color: #444;
  border-top: 1px solid #666;
  border-right: 1px solid #666;
  transform: translateY(-50%) rotate(45deg);
}
.falseMask2 {/*假的遮盖箭头的*/
  position: absolute;
  right: 10rpx;
  top: 0;
  height: 103rpx;
  width: 37rpx;
  background-color: #fff;
  z-index: 2;
}
.addressImg {
  position: absolute;
  top: 50%;
  left: 16px;
  transform: translateY(-50%);
  height: 16px;
  width: 16px;
}

/* 添加常用 */
.flx {
  display: flex;
  justify-content: space-around;
}
.flx_r {
  flex-direction: row;
}
.flx_c {
  flex-direction: column;
}
.modifyPopupcustom {
  border-radius: 30rpx;
  width: 750rpx;
  height: 100vh;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(204, 204, 204, .7);
  z-index: 100;
}
.modifyPopupcustom input {
  color: #333;
}
.modifyPopup {
  position: relative;
  margin: 40% auto;
  padding-bottom: 90rpx;
  height: 500rpx;
  width: 710rpx;
  border-radius: 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  overflow: hidden;
  font-size: 26rpx;
}
.modifyPopupscroll {
  margin: 0 auto;
  padding-bottom: 90rpx;
  height: 100%;
  width: 670rpx;
  background-color: #fff;
}
.modifyRecordTitle {
  margin: 0 auto;
  margin-top: 15px;
  padding: 15rpx 0 0 0;
  font-size: 36rpx;
  color: #18c2ba;
  background-color: #e7f9f8;
  text-align: center;
}

.addNewFactoryHead {
  padding: 10px 0;
  text-align: center;
  background-color: #e7f9f8;
  color: #000;
  font-size: 28rpx;
}
.addNewFactoryHead view:nth-child(1) {
  flex: 1;
}
.addNewFactoryHead view:nth-child(2) {
  flex: 1;
}
.addNewFactoryHead view:nth-child(3) {
  flex: 1;
}
.addNewFactoryHead view:nth-child(4) {
  flex: .4;
}
.modifyRecordbbox {
  margin-top: 20px;
  background: #e7f9f8;
  padding: 9px;
  box-sizing: border-box;
}

.newModify {
  margin: 24rpx auto;
  padding: 20rpx;
  justify-content: space-around;
  background-color: #fff;
  box-sizing: border-box;
  border-radius: 20rpx;
}
.newModify .modifyInputName,.newModify .modifyInputSum {
  /* width: 43%; */
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 24rpx;
  border: 1rpx solid #18c2ba;
}
.modifyInput {
  color: #ccc;
  font-size: 24rpx;
  text-align: center;
}
.modifyPopupBtns {
  position: absolute;
  bottom: 0;
  left: 0;
  height: 90rpx;
  width: 710rpx;
  border-top: 1rpx solid #eaeaea;
  line-height: 90rpx;
  font-size: 30rpx;
  background-color: #fff;
}
.modifyPopupBtns text {
  display: block;
  flex: 1;
  text-align: center;
}
.modifySure {
  box-sizing: border-box;
  border-left: 1rpx solid #eaeaea;
}

.van-rate {
  vertical-align: bottom;
}


/* 选择了的厂家 */
.fll {
  float: left;
}
.flr {
  float: right;
}
.facBox {
  position: relative;
  margin: 15px auto;
  padding: 15rpx;
  width: 730rpx;
  border-radius: 20rpx;
  background-color: #fff;
  box-sizing: border-box;
  font-size: 28rpx;
}
.facTitle {
  height: 50rpx;
  font-size: 28rpx;
  color: #444;
}
.facTips {
  font-size: 24rpx;
  color: #666;
}
.facTitle .flr {
  position: relative;
  margin-right: 40rpx;
}
.facTitle .flr::after {
  content: '';
  position: absolute;
  height: 20rpx;
  width: 20rpx;
  right: -30rpx;
  top: 50%;
  border-top: 1px solid #666;
  border-right: 1px solid #666;
  transform: translateY(-50%) rotate(45deg);
}
.facItem {
  margin: 10px 0;
  min-height: 80rpx;
}
.facInput1,.facInput2 {
  width: 320rpx;
  height: 70rpx;
  border-radius: 10rpx;
  border: 1rpx solid #18c2ba;
  text-align: center;
  color: #444;
}
.facInput2 {
  position: relative;
  margin-left: 70rpx;
  width: 190rpx;
}
.facInput2::after {
  position: absolute;
  content: '手';
  right: 8rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size: 30rpx;
  color: #444;
}
.absdelBox {
  position: relative;
  height: 80rpx;
  width: 50rpx;
  margin-left: 40rpx;
}
.delFac {
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
  display: block;
  height: 40rpx;
  width: 40rpx;
  border-radius: 50%;
  border: 1rpx solid #000;
  text-align: center;
  line-height: 33rpx;
  font-size: 43rpx;
}
/* 假遮罩 */
.falseMask {
  position: fixed;
  z-index: 101;
  width: 750rpx;
  height: 100%;
  background: rgba(0, 0, 0, 0);
}

/* 遮罩 */
.mask9 {
  z-index: 9;
  position: fixed;
  left: 0;
  top: 0;
  height: 100vh;
  width: 750rpx;
  background-color: rgba(204, 204, 204, .7);
}