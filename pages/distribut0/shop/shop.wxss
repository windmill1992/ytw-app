.product_info {
  margin-top: 20rpx;
  margin-bottom: 120rpx;
}
.product_info .product_list {
  position: relative;
  float: left;
  width: 345rpx;
  height: 494rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  margin-left: 20rpx;
  border-radius: 10rpx;
  background: #ffffff;
  overflow: hidden;
}

.product_info .product_list .product_image {
  width: 345rpx;
  height: 345rpx;
  background-color: #eaeaea;
  position: relative;
  overflow: hidden;
}
.product_info .product_list .product_image .product_image_title {
  color: #fff;
  font-size: 60rpx;
  margin-top: 116rpx;
  text-align: center;
}
.product_info .product_list .product_image .product_image_content {
  text-align: center;
  margin-top: 5rpx;
  font-size: 36rpx;
  color: #fff;
  letter-spacing: 5rpx;
}
.product_info .product_list .product_image image {
  position: absolute;
  top: 0;
  left: 0;
  width: 345rpx;
  height: 345rpx;
}
.product_info .product_list .product_list_title {
  font-size: 26rpx;
  color: #333333;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  height: 50rpx;
  line-height: 50rpx;
  width: 320rpx;
  padding-left: 10rpx;
}
.product_info .product_list .activities {
  padding-left: 10rpx;
}
.product_info .product_list .activities .activity {
  float: left;
  padding: 2rpx 6rpx;
  border: 1rpx solid #ff3b3b;
  font-size: 20rpx;
  color: #ff3b3b;
  margin-right: 10rpx;
  border-radius: 3rpx;
}
.product_info .product_list .product_data {
  margin-top: 5rpx;
  padding-left: 10rpx;
  width: 320rpx;
}
.product_info .product_list .product_data .price {
  font-size: 22rpx;
  color: #ff3b3b;
  font-weight: bold;
  float: left;
}
.product_info .product_list .product_data .price > view {
  display: inline;
  font-size: 30rpx;
}
.product_info .product_list .product_data .amount {
  float: left;
  color: #999;
  font-size: 22rpx;
  padding-top: 7rpx;
  /* margin-left: 80rpx; */
}


