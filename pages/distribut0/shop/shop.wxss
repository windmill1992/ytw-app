/* 10.6 操作mask */

.operationMask {
  padding-top: 20vh;
  top: 0;
  left: 0;
  position: fixed;
  height: 100vh;
  width: 750rpx;
  box-sizing: border-box;
  z-index: 1000;
  background-color: rgba(0, 0, 0, .7);
}
.operationItemBox {
  margin: 0 auto;
}
.operationItem {
  margin: 18rpx auto;
  display: flex;
  flex-direction: row;
  padding: 10rpx 0rpx 10rpx 20rpx;
  height: 70rpx;
  width: 550rpx;
  border-radius: 45rpx;
  border: 1rpx solid #18c2ba;
  background-color: #fff;
}
.operationImg {
  margin-right: 8rpx;
  height: 70rpx;
  width: 70rpx;
  border-radius: 50%;
}
.operationImg image {
  height: 70rpx;
  width: 70rpx;
  border-radius: 50%;
}
.operationTxt {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.operationTitle {
  font-weight: 600;
  font-size: 26rpx;
  color: #666;
}
.operationTips {
  font-size: 22rpx;
  color: #aaa;
}
.closeOperationMask {
  position: relative;
  margin: 22rpx auto;
}
.closeOperationMask text {
  position: absolute;
}
.closeOperationMaskX {
  left: 50%;
  transform: translateX(-50%);
  top: 0;
  height: 60rpx;
  width: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  background-color: #18c2ba;
  border-radius: 50%;
}
.closeOperationMaskTxt {
  left: 50%;
  top: 60rpx;
  transform: translateX(-50%);
  color: #18c2ba;
  font-size: 32rpx;
}