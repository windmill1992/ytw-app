}
.addressBox {
  position: relative;
  padding: 8rpx 45rpx 8rpx 195rpx;
  box-sizing: border-box;
  color: #323233;
  background-color: #fff;
}
.addressBox::before {
  content: '收货地址';
  position: absolute;
  left: 16px;
  top: 50%;
  transform: translateY(-50%);
}
.addressBox::after {
  content: '';
  height: 20rpx;
  width: 20rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  border-top: 1px solid #666;
  border-right: 1px solid #666;
  transform: translateY(-50%) rotate(45deg);
}


