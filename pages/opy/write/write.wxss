/* 操作提醒弹窗 */
.warningPopup {
  overflow-y: unset!important;
  border-radius: 0 0 20rpx 20rpx;
}
.warningBox {
  position: relative;
  padding: 80rpx 20rpx 20rpx 30rpx;
  min-height: 450rpx;
  width: 550rpx;
  box-sizing: border-box;
}
.warningBox image {
  position: absolute;
  top: -130rpx;
  left: 0;
  width: 550rpx;
  height: 310rpx;
  z-index: -1;
}
.warningTxt {
  margin: 20rpx 0 10rpx 0;
  color: #18c2ba;
  font-size: 30rpx;
  text-align: center;
  line-height: 55rpx;
}

.warningBtn1 {
  margin: 0 auto;
  width: 85%;
