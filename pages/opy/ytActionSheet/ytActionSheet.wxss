.ytwActionMask {
  position: fixed;
  top: 0;
  left: 0;
  height: 100vh;
  width: 750rpx;
  background-color: rgba(0, 0, 0, .6);
  box-sizing: border-box;
  z-index: 10;
  color: #555;
}
.ytwActionBody {
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 80rpx 0;
  padding-top: 100rpx;
  width: 750rpx;
  box-sizing: border-box;
  background-color: #fff;
  transition: all .3s linear;
}
.roundAction {
  border-radius: 45rpx 45rpx 0 0;
}
.ytwActionCancel,.ytwActionConfirm {
  position: absolute;
  top: 35rpx;
  font-size: 18px;
  color: #999;
}
.ytwActionConfirm {
  padding-right: 40rpx;
  right: 30rpx;
}
.ytwActionCancel {
  left: 30rpx;
}
.absPlus {
  position: absolute;
  right: 12rpx;
  top: 13rpx;
  border-radius: 50%;
  border: 1rpx solid #555;
  height: 20rpx;
  width: 20rpx;
  line-height: 20rpx;
  font-size: 20rpx;
  text-align: center;
}
.ytwActionTitle {
  position: absolute;
  left: 50%;
  top: 15rpx;
  transform: translateX(-50%);
  text-align: center;
  font-weight: 600;
  color: #666;
  font-size: 20px;
}
.ytwActionScroll {
  white-space: nowrap;
  height: 100%;
}
.absSure {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 80rpx;
  border-top: 1rpx solid #eaeaea;
  color: #18c2ba;
  text-align: center;
  line-height: 80rpx;
  font-size: 35rpx;
  font-weight: 600;
}